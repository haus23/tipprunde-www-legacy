{
  "name": "foh",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:css && remix build",
    "build:css": "tailwindcss -m -c ./tailwind.config.cjs -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"pnpm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -c ./tailwind.config.cjs -i ./styles/app.css -o app/styles/app.css"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@remix-run/vercel": "^1.11.1",
    "@vercel/node": "^2.6.2",
    "dtp-types": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "radix-colors-plugin": "workspace:*",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.9.4"
  }
}
